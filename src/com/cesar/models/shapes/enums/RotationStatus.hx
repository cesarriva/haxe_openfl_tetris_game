package com.cesar.models.shapes.enums;

/**
 * Represents the possible states the the rotation of the shapes
 * @author Cesar Riva
 */
enum RotationStatus 
{
	/**
	 * Default position
	 */
	Full;
	/**
	 * First rotation
	 */
	OneQuarter;
	/**
	 * Second rotation
	 */
	Half;
	/**
	 * Third rotation
	 */
	ThreeQuarters;
}