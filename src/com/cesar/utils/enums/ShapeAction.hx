package com.cesar.utils.enums;

/**
 * Define the possible actions for a shape
 * @author Cesar Riva
 */
enum ShapeAction 
{
	/**
	 * Default move, down by one unit 
	 */
	DefaultMove;
	
	/**
	 * Rotate clock-wise movement
	 */
	RotateMove;
	
	/**
	 * Move to the right by one unit
	 */
	RightMove;
	
	/**
	 * Down by one unit
	 */
	DownMove;
	
	/**
	 * Move to the left by one unit
	 */
	LeftMove;
}